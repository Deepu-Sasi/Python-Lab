    "f  = open(\"prime1.txt\", \"r\")\n",
    "print(\"The prime numbers in the file\\n\")\n",
    "for each in f:\n",
    "    number = int(float(each))\n",
    "    if number > 1:\n",
    "        for i in range(2, number):\n",
    "            if (number % i) == 0:\n",
    "                break\n",
    "        else:\n",
    "            print(str(number))\n",
